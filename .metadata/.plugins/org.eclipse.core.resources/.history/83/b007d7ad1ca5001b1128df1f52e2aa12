package homeWork_3;

public class miniProjeAsalSayi {

	public static void main(String[] args) {
		
		
		int number = 25;
		int remainder = number %2;
		System.out.println(remainder);
		boolean isPrime = true;
		
		for(int i=2; i<number;i++) {
			if (number % i ==0) {
				isPrime = false;
			}
		}

	}

}
