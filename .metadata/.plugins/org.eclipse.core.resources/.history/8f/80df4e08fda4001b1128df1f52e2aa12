package homeWork_3;

public class variables {

	public static void main(String[] args) {
		
		//case sensitive : buyuk-kucuk karakterlere duyarli
		
		//int sayi = 10; //camel casing
		int ogrenciSayisi =12; //reusability
		System.out.println("Ogrenci Sayim :"+ogrenciSayisi);
		System.out.println("Ogrenci Sayim : 9");
		System.out.println("Ogrenci Sayim : 9");
		System.out.println("Ogrenci Sayim : 9");
		
		
		System.out.println("Ogrenci Sayim :"+ogrenciSayisi);
		System.out.println("Ogrenci Sayim :"+ogrenciSayisi);
		System.out.println("Ogrenci Sayim :"+ogrenciSayisi);

	}

}
